  // Schema.org
  schemaOrgType          String?
  schemaOrgName          String?
  schemaOrgDescription   String?
  schemaOrgAddress       String?
  schemaOrgCity          String?
  schemaOrgRegion        String?
  schemaOrgPostalCode    String?
  schemaOrgCountry       String?
  schemaOrgTelephone     String?
  schemaOrgPriceRange    String?
  schemaOrgOpeningHours  String?

  // Verification
  googleSiteVerification String?
  bingSiteVerification   String?
  yandexVerification     String?

  // Değerleme Sayfası SEO
  valuationPageTitle       String?
  valuationPageDescription String?
  valuationPageSlug        String?

  // Satış/Kiralama Sayfası SEO
  propertyRequestPageTitle       String?
  propertyRequestPageDescription String?
  propertyRequestPageSlug        String?
